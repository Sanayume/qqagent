# OneBot/NapCat 配置
ONEBOT_WS_URL=ws://127.0.0.1:3001
ONEBOT_REVERSE_WS_HOST=127.0.0.1
ONEBOT_REVERSE_WS_PORT=5140
ONEBOT_REVERSE_WS_PATH=/onebot
ONEBOT_TOKEN=
ONEBOT_MODE=reverse

# LLM 配置
OPENAI_API_KEY=sk-xxx
OPENAI_API_BASE=https://api.openai.com/v1
DEFAULT_MODEL=gpt-4o-mini
GOOGLE_API_KEY=

# Agent 行为
AGENT_DEFAULT_PRESET=default
AGENT_ALLOW_AT_REPLY=true
AGENT_ALLOW_PRIVATE=true
AGENT_ALLOW_ALL_GROUP_MSG=false
AGENT_SILENT_ERRORS=false
AGENT_MAX_HISTORY_MESSAGES=70

# 会话管理
AGENT_SESSION_GLOBAL_USERS=[]
AGENT_SESSION_PER_USER_GROUPS=[]
AGENT_SESSION_ALL_GROUPS_PER_USER=false

# LangSmith 调试 (可选)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=lsv2_pt_xxx
LANGCHAIN_PROJECT=langgraph-qq-agent
